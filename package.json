    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-router-dom": "^6.26.1",
    "strip-ansi": "^7.1.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.3.3",
